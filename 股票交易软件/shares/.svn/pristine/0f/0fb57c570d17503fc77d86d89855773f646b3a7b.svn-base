package com.hxy.isw.entity;
import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.Table;

   /**
    * setting 实体类
    * Fri Jul 14 17:20:34 CST 2017 lxq
    */ 

@Entity
@Table(name = "setting")
public class Setting{

	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	@Column(name = "id")
	private Long id;

	@Column(name = "sharesproportion")
	private Float sharesproportion;

	@Column(name = "foreignexchangeproportion")
	private Float foreignexchangeproportion;

	@Column(name = "purchasegold")
	private Integer purchasegold;

	@Column(name = "viewgold")
	private Integer viewgold;

	@Column(name = "subscriptiongold")
	private Integer subscriptiongold;

	@Column(name = "plansuccess")
	private Float plansuccess;

	@Column(name = "planfail")
	private Float planfail;

	@Column(name = "returnuser")
	private Float returnuser;

	@Column(name = "invitefriends")
	private Integer invitefriends;

	@Column(name = "friends")
	private Integer friends;

	@Column(name = "createtime")
	private Date createtime;

	public void setId(Long id){
	this.id=id;
	}
	public Long getId(){
		return id;
	}
	public void setSharesproportion(Float sharesproportion){
	this.sharesproportion=sharesproportion;
	}
	public Float getSharesproportion(){
		return sharesproportion;
	}
	public void setForeignexchangeproportion(Float foreignexchangeproportion){
	this.foreignexchangeproportion=foreignexchangeproportion;
	}
	public Float getForeignexchangeproportion(){
		return foreignexchangeproportion;
	}
	public void setPurchasegold(Integer purchasegold){
	this.purchasegold=purchasegold;
	}
	public Integer getPurchasegold(){
		return purchasegold;
	}
	public void setViewgold(Integer viewgold){
	this.viewgold=viewgold;
	}
	public Integer getViewgold(){
		return viewgold;
	}
	public void setSubscriptiongold(Integer subscriptiongold){
	this.subscriptiongold=subscriptiongold;
	}
	public Integer getSubscriptiongold(){
		return subscriptiongold;
	}
	public void setPlansuccess(Float plansuccess){
	this.plansuccess=plansuccess;
	}
	public Float getPlansuccess(){
		return plansuccess;
	}
	public void setPlanfail(Float planfail){
	this.planfail=planfail;
	}
	public Float getPlanfail(){
		return planfail;
	}
	public void setReturnuser(Float returnuser){
	this.returnuser=returnuser;
	}
	public Float getReturnuser(){
		return returnuser;
	}
	public void setInvitefriends(Integer invitefriends){
	this.invitefriends=invitefriends;
	}
	public Integer getInvitefriends(){
		return invitefriends;
	}
	public void setFriends(Integer friends){
	this.friends=friends;
	}
	public Integer getFriends(){
		return friends;
	}
	public void setCreatetime(Date createtime){
	this.createtime=createtime;
	}
	public Date getCreatetime(){
		return createtime;
	}
}

