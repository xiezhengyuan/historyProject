	@Column(name = "virtualcapital")
	private Double virtualcapital;

	@Column(name = "totalrecharge")
	private Double totalrecharge;

	@Column(name = "email")
	private String email;

	@Column(name = "address")
	private String address;

	@Column(name = "bank")
	private String bank;

	@Column(name = "bankaccount")
	private String bankaccount;

	@Column(name = "cardno")
	private String cardno;

	@Column(name = "paparno")
	private String paparno;

	@Column(name = "createtime")
	private Date createtime;

	@Column(name = "state")
	private Integer state;

	@Column(name = "speech")
	private Integer speech;

	@Column(name = "faccountid")
	private Long faccountid;

	@Column(name = "finvateid")
	private Long finvateid;

	@Column(name = "isexample")
	private Integer isexample;

	public void setId(Long id) {
		this.id = id;
	}

	public Long getId() {
		return id;
	}

	public void setUsername(String username) {
		this.username = username;
	}

	public String getUsername() {
		return username;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public String getPassword() {
		return password;
	}

	public void setMobile(String mobile) {
		this.mobile = mobile;
	}

	public String getMobile() {
		return mobile;
	}

	public void setNickname(String nickname) {
		this.nickname = nickname;
	}

	public String getNickname() {
		return nickname;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public String getHeadimg() {
		return headimg;
	}

	public void setHeadimg(String headimg) {
		this.headimg = headimg;
	}

	public String getIntro() {
		return intro;
	}

	public void setIntro(String intro) {
		this.intro = intro;
	}

	public void setWxid(String wxid) {
		this.wxid = wxid;
	}

	public String getWxid() {
		return wxid;
	}

	public void setGolds(Double golds) {
		this.golds = golds;
	}

	public Double getGolds() {
		return golds;
	}

	public void setVirtualcapital(Double virtualcapital) {
		this.virtualcapital = virtualcapital;
	}

	public Double getVirtualcapital() {
		return virtualcapital;
	}

	public Double getTotalrecharge() {
		return totalrecharge;
	}

	public void setTotalrecharge(Double totalrecharge) {
		this.totalrecharge = totalrecharge;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getEmail() {
		return email;
	}

	public void setAddress(String address) {
		this.address = address;
	}

	public String getAddress() {
		return address;
	}

	public void setBank(String bank) {
		this.bank = bank;
