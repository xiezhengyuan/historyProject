	width: 165px;
	height: 36px;
	overflow: hidden;
	display: inline-block;
}

#select_cl.dropdown {
	background: #666666;
}

#select_cl.dropdown.open {
	background: #999999;
}

#select_cl .dropdown-menu {
	border: none;
	border-radius: 0;
	padding: 0;
	margin: 0;
