/* bootstrap customize */

input[type="text"], input[type="password"], input[type="file"], textarea {
	box-shadow: none !important;
	border-radius: 2px !important;
}

input[disabled] {
	background: #DDDDDD;
	border: none;
}

input[type="text"]:hover, input[type="text"]:focus, input[type="password"]:hover, input[type="password"]:focus, textarea:hover, input[type="file"]:focus, textarea:focus {
	border-color: #FF6B00 !important;
}

li {
	list-style-type: none !important;
}

h1 {
	font-size: 200%;
	font-weight: normal !important;
}

h2, h3, h4 {
	font-weight: normal !important;
}

h2 .small, h3 .small {
	margin-left: 20px;
}

h3.small {
	font-size: 18px;
	color: #333333;
}

.input-prepend .add-on:first-child, .input-prepend .btn:first-child {
	border-radius: 2px 0 0 2px !important;
}

.table th, .table td {
	text-align: center !important;
}

.control-group .controls a[role="button"] {
	margin-left: 0;
}

.controls-row [class*="span"] + [class*="span"] {
	margin-left: 10px;
}

.modal {
	border-radius: 3px;
	background-color: #FF6B00;
}

.modal-header {
	border: none;
	color: #FFFFFF;
}

.modal-body {
	background-color: #FFFFFF;
}

.modal-footer {
	background-color: #FFFFFF;
	border: none;
	border-radius: 0 0 3px 3px;
	box-shadow: none;
}

.label, .badge {
	padding: 3;
	margin-left: 10px;
	font-size: 12px;
	border-radius: 0;
}

.progress {
	border-radius: 0;
	box-shadow: 0 -1px 0 #DDDDDD inset;
	background: #EEEEEE;
	height: 5px;
}

.progress .bar {
	background: #00A0DF;
	box-shadow: 0 -1px 0 #0084B7 inset;
}

.warning {
	color: #CC0000 !important;
}

.success {
	color: #27AE60;
}

.warning:hover {
	color: #CC0000;
}

.label-success {
	background-color: #27AE60;
}

.input-nano {
	width: 20px;
}

/* body */

body {
	background: #EEEEEE !important;
	font-family: "Helvetica Neue", Helvetica, "Microsoft YaHei", "\5FAE\8F6F\96C5\9ED1", Arial, sans-serif;
}

.left {
	float: left;
}

.right {
	float: right;
}

.bold {
	font-weight: bold;
}

.input-tiny {
	width: 30px;
}

.btn_transparent {
	color: #BBBBBB;
}

.btn_transparent:hover {
	color: #FFFFFF;
	text-decoration: none;
}

.btn_logo_orange, .btn_logo_blue, .btn_red, .btn_green {
	border-radius: 12px;
	padding-top: 3px;
	padding-bottom: 25px;
	border: none;
	color: #FFFFFF;
	text-align: center;
}

.btn_logo_orange:hover, .btn_logo_blue:hover, .btn_red:hover {
	box-shadow: none;
	position: relative;
	top: 2px;
	text-decoration: none;
	color: #FFFFFF;
}

.btn_logo_orange {
	background: #FF6B00;
	box-shadow: 0 -2px 0 #A84700 inset;
}

.btn_logo_blue {
	background: #009FDD;
	box-shadow: 0 -2px 0 #0084B7 inset;
}

.btn_green {
	background: #009910;
	box-shadow: 0 -2px 0 #00660B inset;
}

.btn_red {
	background: #CC0000;
	box-shadow: 0 -2px 0 #870000 inset;
}

.btn_white {
	background: #EEEEEE;
	box-shadow: 0 -2px 0 #DDDDDD inset;
	border-radius: 2px;
	padding: 6px 0;
	border: none;
	color: #333333;
}

.btn_white:hover{
	box-shadow: none;
	position: relative;
	top: 2px;
	text-decoration: none;
	color: #333333;
}

.btn_white_visited {
	background-color: #EEEEEE;
	box-shadow: 0 -2px 0 #DDDDDD inset;
}

.btn_disable {
	text-align: center;
	background: #DDDDDD;
	box-shadow: 0 -2px 0 #CCCCCC inset;
	border-radius: 2px;
	padding: 6px 0;
	border: none;
	color: #888888;
}

.btn_disable:hover{
	text-decoration: none;
	color: #888888;
}

.btn_large {
	padding: 12px 20px;
	font-size: 120%;
}

.btn_update {
	padding: 12px 10;
	width: 220px !important;
}

.btn_h2 {
	font-size: 16px;
	margin: 10px 0 10px 15px;
	padding: 0 15px;
	line-height: 36px;
}

.btn_h3 {
	font-size: 14px;
	margin: 12px 0 12px 15px;
	padding-left: 15px;
	padding-right: 15px;
}

.btn_h3_hint {
	display: inline;
	line-height: 20px;
	margin: 16px 0;
}

.color_logo_orange {
	color: #FF6B00;
}

.color_logo_blue {
	color: #009FDD;
}

.color_gray {
	color: #999999;
}

.dash {
	border-top: 2px solid #FF6B00;
	position: relative;
	top: 14px;
}

.small {
	font-size: 75%;
	color: #999999;
}

.form_hint {
	padding: 6px 0;
}

.modal-footer input[type="button"] {
	padding: 8px 15px;
	margin-left: 15px;
}

.error {
	color: #B94A48 !important;
}

.tag, .tag_auto {
