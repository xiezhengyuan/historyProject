}

.title_underline {
	border-bottom: 1px solid #DDDDDD;
}

.label-info, .badge-info {
	background-color: #009FDD;
}

.two_column {
	float: left;
	overflow-x: hidden;
	background: #FFFFFF;
	padding: 0 !important;
}

.two_column_inner {
	float: left;
	background: #F5F5F5;
	position: relative;
	right: 67%;
